  <definition class="org.jenkinsci.plugins.workflow.cps.CpsFlowDefinition" plugin="workflow-cps@2.57.2">
    <script>pipeline {
    agent {
       label 'Cloud-Native-Pointfix'
    }
    stages {
        stage('test') {
            steps {
                script {
                    if (env.REPO =="eric-enm-sles" || env.REPO == "eric-enm-sg") {
                        when {
                           anyOf {
                               allOf {
                                   not { expression { env.REPO == "eric-enm-sls" } }
                                   expression { env.Chart == "eric-enm-auto" }
                                }
                                expression { env.REPO == "eric-enm-sg" }
                            }
                
                        }
                    }
                echo "${REPO}"
                echo "${Chart}"
                }
            }
        }
    }
}</script>
    <sandbox>true</sandbox>
  </definition>
