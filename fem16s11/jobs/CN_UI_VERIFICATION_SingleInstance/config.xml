tar -zxf ${tarFileName}

echo &quot;tafVersion=${taf_version}&quot; &gt;&gt; &quot;${WORKSPACE}/parameters.properties&quot;

sh MTELoopScripts/pipeline_scripts/ui_launcher_link_verification_setup.sh &quot;${WORKSPACE}/parameters.properties&quot; || exit 1</command>
