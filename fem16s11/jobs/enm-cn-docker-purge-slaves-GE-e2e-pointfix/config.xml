  <definition class="org.jenkinsci.plugins.workflow.cps.CpsFlowDefinition" plugin="workflow-cps@2660.vb_c0412dc4e6d">
    <script>#!/usr/bin/env groovy
def nodes = [:]
//Label Docker-POC needs to be replaced with Cloud-Native-Pointfix
nodesByLabel(&apos;Docker-POC&apos;).each {
  nodes[it] = { -&gt;
    node(it) {
      stage(&quot;Purge@${it}&quot;) {
        sh(&apos;docker version &amp;&amp; docker images &amp;&amp; docker system prune -af &amp;&amp; docker images&apos;)
      }
    }
  }
}

nodesByLabel(&apos;INT_Cloud_Native_Pointfix&apos;).each {
  nodes[it] = { -&gt;
    node(it) {
      stage(&quot;Purge@${it}&quot;) {
        sh(&apos;docker version &amp;&amp; docker images &amp;&amp; docker system prune -af &amp;&amp; docker images&apos;)
      }
    }
  }
}

parallel nodes




</script>
    <sandbox>true</sandbox>
  </definition>
