	&lt;suites&gt;NSCS_GENERATE_ENROLLMENT_INFO_Ciphers.xml&lt;/suites&gt;
&lt;/item&gt;

&lt;/item-group&gt;
</defaultValue>
          <trim>false</trim>
        </hudson.model.TextParameterDefinition>
      </parameterDefinitions>
