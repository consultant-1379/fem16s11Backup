          <name>Testware_Items</name>
          <defaultValue>&lt;item-group parallel=&quot;true&quot;&gt;


&lt;item timeout-in-seconds=&quot;2400&quot;&gt;
&lt;name&gt;NSCS_TRUST_EXTCAIPSEC - Ciphers&lt;/name&gt;
&lt;component&gt;com.ericsson.nms.security.nscs:ERICTAFnodesecurity_CXP9032282&lt;/component&gt;
&lt;suites&gt;NSCS_TRUST_EXTCAIPSEC_Ciphers.xml&lt;/suites&gt;
