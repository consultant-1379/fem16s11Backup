  <definition class="org.jenkinsci.plugins.workflow.cps.CpsFlowDefinition" plugin="workflow-cps@2660.vb_c0412dc4e6d">
    <script>pipeline {
    agent { 
        label &apos;Docker-POC&apos; 
        //label &apos;Cloud-Native&apos;
    }
    stages {
        stage(&apos;Build environment file&apos;) {
            steps {
                script {
                    deleteDir()
                    env.getEnvironment().each { key, value -&gt;
                        if (&quot;${key}&quot;.startsWith(&quot;ERIC_&quot;)){
                            sh &quot;echo &apos;${key}=${value}&apos; &gt;&gt; enm_artifact.properties&quot;
                        }
                    }
                    sh &quot;echo &apos;ERIC_ENM_MODELS_IMAGE_TAG=1.2.3&apos; &gt;&gt; enm_artifact.properties&quot;
                    archiveArtifacts artifacts: &apos;enm_artifact.properties&apos;, fingerprint: true
                }
            }
        }
    }
}
</script>
    <sandbox>true</sandbox>
  </definition>
