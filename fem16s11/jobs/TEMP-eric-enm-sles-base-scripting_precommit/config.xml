      <userRemoteConfigs>
        <hudson.plugins.git.UserRemoteConfig>
          <refspec>$GERRIT_REFSPEC:$GERRIT_REFSPEC</refspec>
          <url>ssh://lciadm100@gerritmirror-ha.lmera.ericsson.se:29418/OSS/com.ericsson.oss.containerisation/eric-enm-sles-base-scripting</url>
          <credentialsId>lciadm100_private_key</credentialsId>
