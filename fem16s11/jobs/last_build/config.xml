      <scriptText>import groovy.json.JsonSlurper;
 
def jsonSlurper = new JsonSlurper()
 
File project_fle = new File(WORKSPACE + &quot;/workspace/&quot; + JOB_NAME + &apos;/jenkins/project_file.json&apos;) 
def project_file_parsed = jsonSlurper.parse(project_fle)
project_file_parsed.each { entry -&gt;
    project_details_map = [:]
    entry.each { key,value -&gt;
        project_details_map[key] = value
    }
  if (project_details_map[&quot;repo&quot;] == REPO) {
     project_details_map[&quot;E2E_enable&quot;] = val
